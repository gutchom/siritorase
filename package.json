{
  "name": "siritorase",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev",
    "dev:firebase": "firebase emulators:start --import=.cache --export-on-exit=.cache",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "lint": "run-s lint:*",
    "lint:css": "stylelint ./**/*.css",
    "lint:ts": "next lint",
    "fix": "run-s fix:*",
    "fix:css": "stylelint ./**/*.css --fix",
    "fix:ts": "next lint --fix",
    "count": "printf '\\e[36m\\n  This project has\\e[35m %s lines\\e[36m of source code!\\n\\n\\e[m' $(find . -name '*.html' -o -name '*.css' -o -name '*.ts' -o -name '*.tsx' | grep -v node_modules | xargs wc -l | tail -1 | awk '{print $1}')"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "date-fns": "^2.29.1",
    "firebase": "^9.9.1",
    "firebase-admin": "^11.0.1",
    "html-react-parser": "^3.0.1",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "recoil": "^0.7.4",
    "reset-css": "^5.0.1",
    "twitter-api-v2": "^1.12.3",
    "twitter-text": "^3.1.0",
    "vis-data": "^7.1.4",
    "vis-network": "^9.1.2"
  },
  "devDependencies": {
    "@firebase/testing": "^0.20.11",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.6",
    "@types/node": "18.6.3",
    "@types/react": "18.0.15",
    "@types/react-test-renderer": "^18.0.0",
    "@types/twitter-text": "^3.1.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "eslint-plugin-tsc": "^2.0.0",
    "firebase-tools": "^11.4.0",
    "jest": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^26.0.0",
    "ts-jest": "^28.0.7",
    "typescript": "4.7.4"
  }
}
